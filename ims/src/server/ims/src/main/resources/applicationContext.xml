<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

	<bean name="placeholderConfig"
		class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer">
		<property name="location">
			<value>classpath:server.ini</value>
		</property>
	</bean>
	
	<bean name="propertyConfig" class="com.eme.ims.utils.PropertyConfig">
		<constructor-arg index="0" value="server.ini"></constructor-arg>
		<constructor-arg index="1" value="0"></constructor-arg>
	</bean>
	
	<bean name="imsWebClient" class="com.eme.ims.servlet.ImsWebClient">
		<constructor-arg index="0" value="client.ini"></constructor-arg>
	</bean>

</beans>